<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/getDataFragment">

    <fragment
        android:id="@+id/getDataFragment"
        android:name="com.example.fetchapplication.GetDataFragment"
        android:label="fragment_get_data"
        tools:layout="@layout/fragment_get_data" >
        <action
            android:id="@+id/action_getDataFragment_to_displayDataFragment"
            app:destination="@id/displayDataFragment" />
    </fragment>
    <fragment
        android:id="@+id/displayDataFragment"
        android:name="com.example.fetchapplication.DisplayDataFragment"
        android:label="fragment_display_data"
        tools:layout="@layout/fragment_display_data" />
</navigation>